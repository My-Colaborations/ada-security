with "util";
with "el";

project Security is
   Version := "@SECURITY_VERSION@";

   for Library_Name use "security";
   for Library_Dir use "INSTALL/lib";
   for Library_Kind use "LIBRARY_TYPE";

   for Source_Dirs use ("INSTALL/share/ada/adainclude/security");
   for Library_ALI_Dir use "INSTALL/lib/ada/adalib/security/" & Project'Library_Kind;
   for Externally_Built use "true";
end Asf;
