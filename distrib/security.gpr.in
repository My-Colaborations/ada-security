with "util";
with "util_config";

library project Security is
   Version := "@SECURITY_VERSION@";

   for Library_Name use "ada_security";
   for Library_Dir use Util_Config.Libdir;
   for Library_Kind use "LIBRARY_TYPE";

   for Source_Dirs use (Util_Config.Includedir & "/ada-security");
   for Library_ALI_Dir use Util_Config.Alidir & "/ada-security/";
   for Externally_Built use "true";
end Security;
