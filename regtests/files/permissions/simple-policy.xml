<policy-rules>
   <security-role>
        <role-name>developer</role-name>
   </security-role>
   <security-role>
        <role-name>manager</role-name>
   </security-role>
   <security-role>
        <role-name>admin</role-name>
  </security-role>
  <role-permission>
    <name>admin</name>
    <role>admin</role>
    <role>manager</role>
  </role-permission>
  <url-policy id='1'>
    <permission>admin</permission>
    <url-pattern>/admin/.*</url-pattern>
  </url-policy>
</policy-rules>
